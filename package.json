{
  "name": "tstok",
  "version": "1.0.2",
  "description": "A TypeScript library and CLI tool for recording live TikTok sessions",
  "main": "dist/lib/index.js",
  "types": "dist/lib/index.d.ts",
  "bin": {
    "tstok": "dist/lib/cli.js"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "start": "node dist/lib/cli.js",
    "dev": "ts-node src/lib/cli.ts",
