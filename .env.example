# TikTok Authentication Cookies
# Get these from your browser when logged into TikTok
TIKTOK_SESSION_ID=your_session_id_here
TIKTOK_TARGET_IDC=useast2a

# Optional: HTTP Proxy (for geo-restricted countries)
# Format: http://username:password@host:port or http://host:port
TIKTOK_PROXY=http://127.0.0.1:8080

# Optional: Default output directory for recordings
TIKTOK_OUTPUT_DIR=./recordings/

# Optional: Telegram Configuration for uploading recordings
TELEGRAM_API_ID=your_api_id
TELEGRAM_API_HASH=your_api_hash
TELEGRAM_BOT_TOKEN=your_bot_token
TELEGRAM_CHAT_ID=1234567890